{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "inlineSourceMap": true,
    "typeRoots": [
      "../Schema/"
    ],
    "paths": {
      "@library/*": [
        "./Lib/*"
      ]
    }
  },
  "include": [
    "./Schema/**/*.d.ts",
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
