<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- "ContentControlThemeFontFamily" is used for the font of the content of a ContentControl. -->
  <FontFamily x:Key="ContentControlThemeFontFamily"> /Assets/Pixel-Millennium.ttf#Pixel Millennium </FontFamily>
  <FontFamily x:Key="ExtraSmallFontFamily"> /Assets/Pixel-rocks.ttf#Pixel rocks </FontFamily>
  <FontFamily x:Key="SmallFontFamily"> /Assets/Pixel-Millennium.ttf#Pixel Millennium </FontFamily>
  <FontFamily x:Key="NormalFontFamily"> /Assets/dogicapixel.ttf#Dogica Pixel </FontFamily>
  <FontFamily x:Key="LargeFontFamily"> /Assets/FSEX300.ttf#Fixedsys Excelsior 3.01 </FontFamily>

</ResourceDictionary>
