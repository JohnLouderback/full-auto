{
  "private": true,
  "engines": {
    "yarn": ">=1.7.0 <2",
    "node": ">=18"
  },
  "scripts": {
    "build:electron": "yarn --cwd electron-app bundle",
    "publish:electron": "yarn --cwd electron-app package",
    "prepare": "lerna run prepare",
    "postinstall": "./scripts/link-packages.bat && node scripts/replace-electron.js && node scripts/patch-app-manager-check.js && theia check:theia-version",
    "start:electron": "yarn --cwd electron-app start",
    "watch:electron": "lerna run --parallel watch --ignore browser-app"
  },
  "devDependencies": {
    "lerna": "2.4.0"
  },
  "workspaces": [
    "script-ed",
    "script-ed-ts-vfs",
    "script-ed-ts-vfs-plugin",
    "browser-app",
    "electron-app"
  ],
  "dependencies": {
    "adm-zip": "^0.5.16"
  },
  "resolutions": {
    "ffi-napi/node-addon-api": "^8.3.1",
    "ref-napi/node-addon-api": "^8.3.1",
    "node-addon-api": "^8.3.1"
  }
}